CC = cc
CFLAGS = -Wall -Wextra -Werror
NAME = libft.a 
AR = ar
SRC =   

OBJECTS = ${SRC:.c=.o}

all: $(NAME)

$(NAME):$(OBJECTS)
	ar rc $(NAME) $(OBJECTS)

%.o: %.c 
	${CC} -c ${CFLAGS} -I. $< -o $@

clean:
	rm -rf $(OBJECTS)

fclean: clean
	rm -rf $(NAME)

re: fclean all
.PHONY = all clean fclean re